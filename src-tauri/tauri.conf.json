{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "tauri-leptos-android",
  "version": "0.1.0",
  "identifier": "com.hexdecima.example",
  "build": {
    "frontendDist": "../src-ui",
    "devUrl": "http://localhost:3007",
    "beforeDevCommand": "trunk serve --port 3007 --open",
    "beforeBuildCommand": "trunk build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-leptos-android",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
